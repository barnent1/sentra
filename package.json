    "jsdom": "^27.2.0",
    "playwright": "^1.48.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "supertest": "^7.1.4",
    "tailwindcss": "^3.4.12",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.6",
    "typescript": "^5.6.2",
    "vitest": "^4.0.8"
  }
}
