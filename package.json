{
  "name": "quetrex",
  "version": "0.1.0",
  "description": "AI Agent Control Center - Voice-First AI Assistant",
  "author": "Glen Barnhardt",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3007",
    "dev:safe": "./scripts/dev-server.sh start",
    "dev:stop": "./scripts/dev-server.sh stop",
    "dev:restart": "./scripts/dev-server.sh restart",
    "dev:status": "./scripts/dev-server.sh status",
    "dev:cleanup": "./scripts/dev-server.sh cleanup",
    "dev:logs": "./scripts/dev-server.sh logs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css,md}\"",
    "deploy:vercel": "vercel --prod",
    "deploy:netlify": "netlify deploy --prod",
    "version:patch": "./scripts/bump-version.sh patch",
    "version:minor": "./scripts/bump-version.sh minor",
    "version:major": "./scripts/bump-version.sh major",
    "release:prepare": "./scripts/bump-version.sh",
    "test": "vitest",
    "test:run": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:debug": "playwright test --debug",
    "generate:e2e": "tsx src/cli/generate-e2e-tests.ts",
    "db:migrate": "prisma migrate dev",
    "db:seed": "tsx prisma/seed.ts",
    "db:reset": "prisma migrate reset",
    "drizzle:generate": "drizzle-kit generate",
    "drizzle:migrate": "drizzle-kit migrate",
    "drizzle:push": "drizzle-kit push",
    "drizzle:studio": "drizzle-kit studio",
    "supabase:migrate": "tsx scripts/migrate-supabase.ts",
    "supabase:setup": "./scripts/setup-supabase.sh"
  },
  "prisma": {
    "seed": "tsx prisma/seed.ts"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "@neondatabase/serverless": "^1.0.2",
